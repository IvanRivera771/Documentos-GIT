Algoritmo Promedio
	
	Definir nota1, nota2, nota3, nota4, prom Como Real;
	
	Escribir "Ingrese la nota del primer Estudiante "
	leer nota1;
	Escribir "Ingrese la nota del segundo Estudiante"
	leer nota2;
	Escribir "Ingrese la nota del tercer Estudiante "
	leer nota3;
	Escribir "Ingrese la nota del cuarto Estudiante"
	leer nota4;
	
	prom <- (not1+nota2+nota3+nota4)/4;
	
	Escribir " El promedio de las notas de los estudiantes es: ", prom;
	
FinAlgoritmo
